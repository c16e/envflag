{
  "name": "Envflag",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Indicate development, staging or production site by simple icon",
  "homepage_url": "http://craftsman-software.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon_unknown.png",
    "default_title": "envflag",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*/* "
  ]
}